
python ./apsviz_station_products.py --url "https://apsviz-thredds-dev.apps.renci.org/thredds/dodsC/2022/nam/2022060800/NCSC_SAB_v1.23/hatteras.renci.org/ncsc123-nam-sb55.01/namforecast/fort.63.nc" --ndays -4  --return_sample_min 60 --gridname 'ncsc_sab_v1.23' --fort63_style --ensemble='namforecast'  --map_file './supporting_data/grid_to_stationfile_maps.yml' --contrails_auth ./secrets/contrails.yml --finalDIR "./HEY3"

